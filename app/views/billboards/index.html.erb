
<h1>Click a Billboard to view details</h1>


<div class="ui massive relaxed divided list">
<% @billboards.each do |billboard| %>
  <div class="item">
    <i class="large github middle aligned icon"></i>
    <div class="content">
      <a class="header"><%= link_to billboard.name, billboard %></a>
      <div class="description">Click to view</div>
    </div>
  </div>
  <% end %>
</div>



